@article{Evans2017,
abstract = {This study assesses whether men's ART use mitigates HIV-risk within age-disparate partnerships. Using data from the 2012 South African National HIV survey, we analyzed differences in HIV prevalence and ART use between men in age-disparate and age-similar partnerships with young women aged 15–29 using multiple logistic regression analyses. Within partnerships involving women 15–24 years old, men in age-disparate partnerships were more likely to be HIV-positive (5–9 year age-gap: aOR 2.8, 95{\%}CI 1.4–5.2; p {\textless} 0.01; 10+ year age-gap: aOR 2.2, 95{\%}CI 1.0–4.6; p {\textless} 0.05). Men in age-disparate partnerships who were 5–9 years older were significantly more likely to be HIV-positive and ART-na{\"{i}}ve (aOR 2.4, 95{\%}CI 1.2–4.8; p {\textless} 0.05), while this was not the case for men 10+ years older (aOR 1.5, 95{\%}CI 0.7–3.6; p = 0.32). No evidence was found that 25–29 year old women were at greater HIV-risk in age-disparate partnerships. Our results indicate that young women aged 15–24 have a greater likelihood of exposure to HIV through age-disparate partnerships, but ART use among men 10+ years older could mitigate risk.},
author = {Evans, Meredith and Maughan-Brown, Brendan and Zungu, Nompumelelo and George, Gavin},
doi = {10.1007/s10461-017-1741-6},
file = {:home/dalonso/My{\_}Documents/Work/My{\_}Papers/19/2019{\_}R{\_}0{\_}AIDS{\_}MODEL/PDF{\_}Papers/Biobliografia-XValles/evans2017.pdf:pdf},
issn = {15733254},
journal = {AIDS and Behavior},
keywords = {Age-disparate partnerships,HIV,Intergenerational partnerships,Sexual behaviour,South Africa},
mendeley-groups = {HIV-AIDS-Models,HIV-AIDS-Madagascar},
number = {8},
pages = {2533--2542},
publisher = {Springer US},
title = {{HIV Prevalence and ART Use Among Men in Partnerships with 15–29 Year Old Women in South Africa: HIV Risk Implications for Young Women in Age-Disparate Partnerships}},
volume = {21},
year = {2017}
}

@article{Eaton2011,
abstract = {The combination of long-term concurrent sexual partnerships and high infectiousness early in HIV infection has been suggested as a key driver of the extensive spread of HIV in general populations in sub-Saharan Africa, but this has never been scientifically investigated. We use a mathematical model to simulate HIV spreading on sexual networks with different amounts of concurrency. The models show that if HIV infectiousness is constant over the duration of infection, the amount of concurrency has much less influence on HIV spread compared to when infectiousness varies over three stages of infection with high infectiousness in the first months. The proportion of transmissions during primary infection is sensitive to the amount of concurrency and, in this model, is estimated to be between 16 and 28{\%} in spreading epidemics with increasing concurrency. The sensitivity of epidemic spread to the amount of concurrency is greater than predicted by models that do not include primary HIV infection. {\textcopyright} 2010 Springer Science+Business Media, LLC.},
author = {Eaton, Jeffrey W. and Hallett, Timothy B. and Garnett, Geoffrey P.},
doi = {10.1007/s10461-010-9787-8},
file = {:home/dalonso/My{\_}Documents/Work/My{\_}Papers/19/2019{\_}R{\_}0{\_}AIDS{\_}MODEL/PDF{\_}Papers/Eaton{\_}2010.pdf:pdf},
